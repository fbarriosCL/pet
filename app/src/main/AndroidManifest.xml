<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felipe.pet_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OwnerActivity" />
        <activity android:name=".PetActivity" />
        <activity
            android:name=".ShowOwnerActivity"
            android:label="@string/title_activity_show_owner"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShowPetActivity"
            android:label="@string/title_activity_show_pet"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>